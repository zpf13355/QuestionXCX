<view class="tabs">
    <view class="title">
        <view wx:for="{{tabs}}" wx:key="id" class="title_item  {{item.isActive?'active':''}}" bindtap="handleTap" data-index="{{index}}"> 
            {{item.value}}
        </view>

    </view>
    <view class="content">
        <slot></slot>
    </view>

</view>
